{
  "name": "card_game",
  "version": "1.0.0",
  "description": "server-side render",
  "main": "index.js",
  "scripts": {
    "server-build": "webpack --config webpack.server.config.js",
    "v4_build": "webpack --config webpack.build.config.js",
    "v3_build": "webpack --config webpack.config.js",
    "build": "./node_modules/.bin/webpack",
    "v1_build": "node ./node_modules/webpack/bin/webpack.js -w",
    "v0_build": "node ./node_modules/webpack/bin/webpack.js -w",
    "start": "concurrently \"npm run build\" \"webpack -w\"  \"nodemon server.js\"",
    "v9_start": "webpack-dev-server",
    "start: web-dev-server": "command: node server.js, not npm start",
    "v8_start": "./node_modules/.bin/webpack && node-dev .",
    "v7_start": "concurrently \"npm run build\" \"nodemon --ext js,jsx\"",
    "v6_start": "concurrently \"npm run build\" \"nodemon server.js\"",
    "v5_start": "./node_modules/.bin/webpack -w && nodemon server.js",
    "v4_start": "concurrently \"npm run build\" \"webpack -w\"  \"nodemon server.js\"",
    "wrong_start": "npm run build && nodemon server.js",
    "v3_start": "webpack -w && nodemon server.js",
    "v2_start": "webpack -w && nodemon --ext js,jsx --ignore public/ --ignore index.js",
    "v1_start": "npm run build && ./node_modules/.bin/node-dev index.js",
    "v0_start": "concurrently \"npm run build\" \"http-server -c-1 -p 8000\" ",
    "explain": "server-side node express is a server no more http-server"
  },
  "author": "Quan Long",
  "license": "ISC",
  "keywords": [
    "react.js",
    "node.js",
    "express.js"
  ],
  "babel": {
    "plugins": [
      "transform-react-jsx",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "react"
    ],
    "ignore": [
      "js/bundle.js",
      "node_modules/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.1.0",
    "css-loader": "^3.0.0",
    "history": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "node-dev": "^4.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "path": "^0.12.7",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "babel-core": "^6.3.21",
    "babel-loader": "^6.4.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "express-validator": "^6.1.1",
    "graphql": "^14.3.1",
    "hbs": "^4.0.4",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
