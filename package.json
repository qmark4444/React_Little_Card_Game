{
  "name": "card_game",
  "version": "1.0.0",
  "description": "server-side render",
  "main": "index.js",
  "scripts": {
    "conditional": "if test \"$NODE_ENV\" = \"production\" ; then ; fi",
    "build": "./node_modules/.bin/webpack",
    "start": "concurrently \"npm run build\" \"webpack -w\"  \"nodemon server.js\""
  },
  "author": "Quan Long",
  "license": "ISC",
  "keywords": [
    "react.js",
    "node.js",
    "express.js"
  ],
  "babel": {
    "plugins": [
      "transform-react-jsx",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "react"
    ],
    "ignore": [
      "css/sass-bundle.css",
      "/server.js",
      "js/bundle.js",
      "node_modules/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.1.0",
    "css-loader": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.8.0",
    "node-dev": "^4.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-register": "^6.26.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "express-validator": "^6.1.1",
    "graphql": "^14.3.1",
    "hbs": "^4.0.4",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "react-router": "^4.2.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1"
  }
